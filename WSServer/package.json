{
	"private": true,
	"name": "ws-server",
	"version": "0.16.0",
	"description": "npm init template for bootstrapping an empty Colyseus project",
	"main": "build/index.js",
	"scripts": {
		"tsx": "tsx watch src/index.ts",
		"loadtest": "tsx test/loadtest/example.ts --room app-room --numClients 2",
		"build": "tsc",
		"test": "mocha -r tsx ts-node/register test/**_test.ts --exit --timeout 15000",
		"schema-codegen": "schema-codegen src/rooms/schema/* --csharp --output ../Assets/Example/",

		"list": "pm2 ls",
		"start": "pm2 start ecosystem.config.js",
		"stop": "pm2 stop ws-server",
		"restart": "pm2 restart ws-server"
	},
	"author": "",
	"license": "ISC",
